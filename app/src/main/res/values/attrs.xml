<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--视差控件ImageView-->
    <declare-styleable name="ParallaxImageView">
        <attr name="a_in" format="float" />
        <attr name="a_out" format="float" />
        <attr name="x_in" format="float" />
        <attr name="x_out" format="float" />
        <attr name="y_in" format="float" />
        <attr name="y_out" format="float" />
    </declare-styleable>

    <!--圆形计时进度条-->
    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RatioImageView">
        <attr name="origin_width" format="integer"/>
        <attr name="origin_height" format="integer"/>
    </declare-styleable>

</resources>